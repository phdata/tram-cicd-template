#!/bin/bash
set -eu -o pipefail

JAVA_CMD=""
if [ -z ${JAVA_HOME+""} ];
then
    echo "Using JAVA_HOME: $JAVA_HOME"
    JAVA_CMD=$JAVA_HOME/bin/java
else
    echo "Using default java"
    JAVA_CMD=java
fi


$JAVA_CMD -cp ".:tram-provisioning.jar" -Dlog4j.configurationFile=log4j2.xml -Dloader.main="io.phdata.tram.App" org.springframework.boot.loader.PropertiesLauncher provision --stack $STACK_PATH --ad-groups --validate-sql
